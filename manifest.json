{
  "manifest_version": 3,
  "name": "Push 2 Github",
  "version": "1.0",
  "description": "Push leetcode question and submitted code to your github repository.",
  "permissions": ["activeTab", "scripting","storage"],
  "host_permissions": ["https://leetcode.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo16.png",
      "48": "logo48.png",
      "128": "logo128.png"
    }
  },
  "icons": {
      "16": "logo16.png",
      "48": "logo48.png",
      "128": "logo128.png"
  }
}
